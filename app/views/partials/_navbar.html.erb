<div class="bg-white">
  <% if logged_in? %>
    <Navbar logout="<%= logout_address %>"
            logo="<%= asset_path('opencsg_logo.png') %>"
            avatar="<%= current_user.avatar_url %>"
            star-chain-url="<%= starchain_address %>"
            is-company-user="<%= current_user.company_user? %>"
            company-verified="<%= current_user.company_verified %>"
            phone="<%= current_user.phone %>"
            user-name="<%= current_user.display_name%>"
            is-logged-in="true"
            :december-release="<%= !!feature_flags['december_release'] %>"
            docs-url="<%= docs_url %>"
    />
  <% else %>
    <Navbar logo="<%= asset_path('opencsg_logo.png') %>"
            star-chain-url="<%= starchain_address %>"
            is-logged-in="false"
            login-url="<%= login_url %>"
            docs-url="<%= docs_url %>"
    />
  <% end %>
</div>
